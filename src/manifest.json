{
  "name": "Click get value",
  "description": "アイコンクリック時に開いているページの値を取得",
  "version": "1.0.0",
  "manifest_version": 2,
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "browser_action": {
    "default_title": "",
    "default_popup": "popup.html"
  },
  "content_scripts": [{
    "matches": ["http://*/*", "https://*/*"],
    "js": ["jquery-2.2.0.min.js"]
  }]
}